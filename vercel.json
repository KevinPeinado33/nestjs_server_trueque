{
    "version": 2,
    "env": {
        "PORT": "@port",
        "POSTGRES_DB": "@namedb",
        "POSTGRES_USER": "@userdb",
        "POSTGRES_PASSWORD": "@passwordb",
        "POSTGRES_PORT": "@portdb",
        "POSTGRES_HOST": "@hostdb",
        "JWT_SECRET": "@jwtsecret"
    },
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}